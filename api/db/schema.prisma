generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native"]
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Country {
  id     Int     @id @default(autoincrement())
  name   String?
  region String?
}

model Car {
  id        Int    @id @default(autoincrement())
  make      String
  model     String
  year      Int
  color     String
  mileage   Float
  vinNumber String @unique
}
